{
    "compilerOptions": {
      "target": "es2015",
      "module": "es2015",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "declaration": true,
      "removeComments": true,
      "sourceMap": true,
      "strict": true,
      "strictPropertyInitialization": false,
    },
    "typedocOptions": {    
      "module": "es2015",
      "target": "es6",
      "excludePrivate": "true",
      "theme": "default",
      "mode": "file",
      "includeVersion": "true", 
    }
}
  